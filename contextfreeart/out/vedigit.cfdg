	jj { x 36}
	kk { x 40}
	ll { x 44}
	mm { x 48}
	nn { x 52}
	oo { x 56}
	pp { x 60}
	qq { x 64}
	rr { x 68}
	ss { x 72}
	tt { x 76}
	uu { x 80}
	vv { x 84}
	ww { x 88}
	xx { x 92}
	yy { x 96}
	zz { x 100}
	beginend {x 104.5}

//______digits, punctuation and others

	beginend {x -1.5 y -13}
	empty {x -2 y -13} 
	zero { y -13}
	one { x 4 y -13}
	two { x 8 y -13}
	three { x 12 y -13}
	four { x 16 y -13}
	five { x 20 y -13}
	six { x 24 y -13}
	seven { x 28 y -13}
	eight { x 32 y -13}
	nine { x 36 y -13}
	dot { x 40 y -13}
	colon{ x 44 y -13}
	comma  { x 48 y -13}
	semicolon { x 52 y -13}
	minus { x 56 y -13}
	plus { x 60 y -13}
	equal { x 64 y -13 }
	o_bracket { x 68 y -13}
	c_bracket { x 72 y -13}
	exclam { x 76 y -13}
	quest { x 80 y -13 }
	at { x 84 y -13 }
	bullet { x 88 y -13 }
	degree { x 92 y -13 }
	apostrophe { x 96 y -13 }
	quote { x 100 y -13 }
	beginend {x 104.5 y -13}
}

rule front_sq {
	SQUARE {b .8}
}

rule front {
	lmnt {b .8}
//	front {s .9 b .1}
}

rule back {
	lmnt {b .2}
}

rule sq {
	SQUARE { b .24}
}

rule triang {
	tr { b .24}
}

path tr {
	LINETO {x .5 y .5}
	LINETO {y 1}
	CLOSEPOLY {}
	FILL {}
}

path lmnt {
	MOVETO {x -.5 y .5}
	LINETO {x .5 y .5}
	LINETO {x 1}
	LINETO {x .5 y -.5}
	LINETO {x -.5 y -.5}
	LINETO {x -1}
	CLOSEPOLY {}
	STROKE {width 0}
	FILL {}
}

