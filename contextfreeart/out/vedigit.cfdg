startshape preview 

background {b -1}

rule preview {
	3 * {x - 32 a -.45 h -55 sat .2}
	{4 * { y 8 a -.45 h 55 sat .2}
	title { x 76 y -64 s 1 .55}}
	3 * {s .5 y -22}
	digitveda{}
}

rule digitveda{
	beginend {x -1.5}
	empty {x -2} 
	aa {}
	bb { x 4}
	cc { x 8}
	dd { x 12}
	ee { x 16}
	ff { x 20}
	gg { x 24}
	hh { x 28}
