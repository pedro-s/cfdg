startshape init
background{hue 100 sat 0.27 b -0.8}

rule init
{
rotSpade{alpha -0.4}
init{r -51 s 0.995 b 0.005}
}

rule rotSpade
{
spade{}
rotSpade{s 0.997 r 1 y 0.5}
}

rule rotSpade
{
rotSpade{r -1}
}

rule spade
{
edge{f 90}
center{x 5.5}
edge{x 11}
}

rule center
{
CIRCLE{b 0.8}
center{y 1.83 s 0.9}
}
rule edge
{
CIRCLE{b 0.5}
edge{r 2 y 2 s 0.9}
}