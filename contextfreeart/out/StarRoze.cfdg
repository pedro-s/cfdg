startshape ROZE
background{b -1}

rule ROZE{
	LOGO{}
	//CIRCLE {b 1 s 25 hue 200 sat 0.9 b 1}
}

rule LOGO{
	LNT{}
	LOGO{s 0.99 r 50 a 0.5}
}

rule LNT{
	LINEAS{}
   LNT{s 0.25 x 5 y 5}
}

rule LINEAS{
	LINWE{hue 5 sat 1 b 1}
   LINWE{r 180}
	LINWE{r 90 hue 5 sat 0.5 b 1 s 0.5}
	LINWE{r 270 s 0.15 hue 5 sat 0.25 b 1}
}

rule CIRCA{
	CIRCLE{}
	CIRCA{s 0.95 y 0.5 x 0.5}
}

rule LINWE{
	CIRCA{}
	CIRCA{r 180}
	LINWE{s 0.9 y 0.75 x -0.75}
}