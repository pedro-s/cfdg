startshape squ
background { b -1 }

rule squ {
	3*{x .33333 } sq2 { x -.3333 y -.3333 s .5}
	3*{x .33333 } sq2 { x -.3333 y   0 s .5 }
	3*{x .33333 } sq2 { x -.3333 y  .3333 s .5 }
}

rule sq2 {
	d { s .4 }
	4* { r 90 } squ { s .5 y 1 }
}



rule d { 
	SQUARE { b 1 }
	SQUARE { b -1 s .7} 
}