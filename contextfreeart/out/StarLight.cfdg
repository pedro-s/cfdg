startshape START

background {b -1}

rule CONRAYOS{
    RAYO [s 0.004]
    CONRAYOS [r 50 s 0.9]
}

rule START{
    CONRAYOS[]
    CIRCLE [s 0.006 hue 291 sat .83 b .85]
}

rule RAYO{
    CIRCLE [s 1.5 hue 291 sat .60 b 1]
    CIRCLE [s 1 hue 300 sat .83 b .85]
    SQUARE [s 0.25 hue 50 sat .50 b 1 y 10]
    RAYO [s 0.96 y .1]
}

rule RAYO 75{
    RAYO [y .15 f 10]
}
