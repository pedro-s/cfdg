startshape START
background { b -1 }

rule START {
	SHAPE { }
	START { s 0.52 flip 90 x -0.99999 y -0.142 b 0.1 sat 0.1 hue 1 alpha -0.4}
	START { s 0.52 r 180 flip 90  y 0.15 x -0.309 b 0.2 sat 0.3 hue 5 alpha -0.5}
	START { s 0.52 flip 90   y -0.14  x -0.05 b 0.4 hue 10 sat 0.5 alpha -0.4}
	START { s 0.52 r 180 r 120  x 0.02 y 0.5 b 0.06 hue -10 sat 0.2 alpha -0.6}
}

rule SHAPE {
	TRIANGLE { s 1.3 y 0.08}
	SQUARE { s 1 0.58 x -0.5 }
	TRIANGLE { s 0.669 x -1 y -0.1}
}