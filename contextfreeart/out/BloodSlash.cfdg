startshape thingieMain

background{b -1}

rule thingieMain{
	thingie{r 45}
	thingie{flip 90 r -45}
}

rule thingie{
	SQUARE{h 0 sat 1 b 1 a -0.9}
	thingie{x 1
		r 0.1
		s 0.9}
	thingie{x -1
		r -0.1
		s 0.9}
}

rule thingie 4{thingie{x -0.1 y -0.1}}
rule thingie 4{thingie{x 0.1 y -0.1}}
rule thingie 4{thingie{x -0.1 y 0.1}}
rule thingie 4{thingie{x 0.1 y 0.1}}
rule thingie 4{thingie{s 0.95}}
rule thingie 4{thingie{r 1}}
rule thingie 4{thingie{r -1}}