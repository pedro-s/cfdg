startshape Blocks

CF::Background = [b -1]

shape Blocks {
	loop 50 [x 1] S []
	loop 50 [x 1] Square [y -1]
}

shape S {
	Sr [h 100 sat 0.1]
}

shape Sr
rule {
	Square []
	S [y 1 x 1]
}
rule {
	Square []
	S [y 1 x -2]
}
rule {
	Square []
	S [y 2]
}
rule {
	S [y 1]
}
rule {}

shape Square {
	SQUARE [b 1]
}
