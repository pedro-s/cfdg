startshape starcirc
background {sat 1 b -1   hue 90}

rule starcirc
{
	10*{r 36 }starz2{y .5 b 1 sat 0 hue 320 alpha -1}
}
rule starz2
{
	9000*{r 5.512 s .996 1 1 b -.0015  sat .001 alpha .01 }star2{y .5  s .5 1 1   }
}

rule star2
{
	triangle{  s .3 1.2 1 }
	triangle{  s .3 .7 1 r 90}
	triangle{  s .3 1.2 1 r 180}
	triangle{ s .3 .7 1 r 270}
}
path triangle
{
	MOVETO { x -.05  y  0}	
	LINETO { x  0 y .1 }
	LINETO { x .05 y 0}
	CLOSEPOLY{}

}

