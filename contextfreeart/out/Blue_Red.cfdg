startshape nyoro

rule nyoro{
	SQUARE{ }
	SQUARE{ s .9 b 1 }
	nyoro{ y 1 s .85}
}

rule nyoro 0.1{
	nyoro{ r 100 s .8}
	nyoro{ flip 0 }
}

rule nyoro 0.1{
	nyoro { r -130 s .8}
	nyoro { flip 0 }
}

rule nyoro 0.3{
	nyoro { flip 0 }
	CIRCLE{ x 1 y 1}
	CIRCLE{ x 1 y 1 s .9 h 230 saturation 1 b 1}
}

rule nyoro 0.3{
	nyoro { flip 0 }
	CIRCLE{ x -1 y 1 }
	CIRCLE{ x -1 y 1 s .9 h 0 saturation 1 b 1}
}