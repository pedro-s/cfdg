startshape synergy
background{b -1}

rule synergy
{

	diamond{hue 50 sat 1 b 1}
	diamond{hue 50 sat 1 b 1 x 42}
	30_curve{b 1 r 45 x 21.4 y -11}
	30_curve{b 1 r 225 x 21 y 30.5}
	br_diamond{b 1 size 1.2 x 21 y -2}
	15_curve{b 1 size 1.15 r 45 y 23.5 x 21}
15_curve{b 1 size 1.15 r 225 y -3.7 x 21}
}

rule br_diamond
{
	full_brsquare{r 45}
}

rule full_brsquare
{
	half_brsquare{}
	half_brsquare{r 180 y 14 x 14}
}

rule half_brsquare
{
	15_curve{hue 30 sat 1 b 1}
	15_curve{r 90 x 14}
}

rule diamond
{
	full_square{r 45}
}

rule full_square
{
	half_square{}
	half_square{flip 180 y 14}
}

rule half_square
{
	15_curve{}
	15_curve{r 90 x 14}
}

