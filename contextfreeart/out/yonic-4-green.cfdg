startshape START

background {b -1 sat 1}

rule START {
        Y1 { h 90 }
        Y1 { f 0 y -360 h 90}
}
rule Y1 {
	2000 * {} A { f 0  r 130 sat 1}
	2000* {} A { r 50 sat 1} 
	}

rule A  {
	SQUARE { b 1}
	A {y 1 s 0.9999 r 5 h -.25 sat 1}
}

rule A  {
         SQUARE { b 1 }
	A {y -10 s 0.9999 r -5 h -.25 sat  1}
}

